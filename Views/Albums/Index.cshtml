@model IEnumerable<SpotifyDataClient.Models.Album>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Albums</h2>

<table class="table">
    <tr>
        <th>
            Album Name
        </th>
        <th>
            Release Year
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.releaseYear)
        </td>
    </tr>
}

</table>
